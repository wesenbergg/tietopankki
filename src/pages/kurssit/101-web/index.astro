---
import Layout from "../../../layouts/Layout.astro";
import { getCollection } from "astro:content";

const posts = await getCollection("101-web");
---

<Layout title="Tervetuloa 101 Web kurssille!">
  <main>
    <h1>Web Perusteet</h1>
    <ul>
      {
        posts.map((p) => (
          <li>
            <a href={`${p.slug}`}>{p.data.title}</a>
          </li>
        ))
      }
    </ul>
  </main>
</Layout>
